{
  "name": "@datafe/mdbi-common-tools",
  "version": "1.4.25-beta.4",
  "description": "魔数公共工具仓库",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src --fix",
    "dev": "nodemon --exec npx run build --exec yalc add",
    "build": "rollup -c",
    "test": "ava",
    "pb": "node ./bin/publish.js"
  },
  "repository": {
    "url": "/mdbi-common-tools",
    "type": "git"
  },
  "author": "<123@123>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@ss/mtd-vue": "^0.3.9",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vue/compiler-sfc": "^3.2.22",
    "ava": "^1.2.0",
    "axios": "^0.24.0",
    "colors-console": "^1.0.3",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-rem": "^4.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^1.2.0",
    "js-cookie": "^3.0.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^6.0.1",
    "nodemon": "^1.18.7",
    "prettier": "^2.3.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-typescript2": "^0.31.0",
    "rollup-plugin-vue": "5.x",
    "typescript": "3.x",
    "typescript-eslint-parser": "^21.0.2",
    "vue": "2.6.14",
    "vue-template-compiler": "2.6.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,vue}": [
        "./node_modules/.bin/eslint src --fix",
        "git add ."
      ],
      "*.{json,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ],
    "watch": [
      "src/*",
      "node_modules/*"
    ],
    "ext": "ts tsx",
    "delay": 2500
  },
  "dependencies": {
    "@mtfe/sso-web": "1.1.18-0",
    "@vue/composition-api": "^1.4.1",
    "uuid": "^8.3.2",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "3.5.3"
  }
}
