{
  "name": "talos-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node ./bin/talos_build",
    "test": "echo missing test",
    "lint": "lint-staged",
    "build:code": "react-scripts build",
    "commit": "git cz",
    "dev": "npm start",
    "eject": "react-scripts eject",
    "start": "react-scripts start"
  },
  "dependencies": {
    "@mtfe/sso-web": "1.1.16-2",
    "axios": "^0.18.0",
    "env-cmd": "8.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.3",
    "yorkie": "2.0.0"
  },
  "devDependencies": {
    "@hfe/common-cli-plugin-axios": "latest",
    "@hfe/common-cli-plugin-flow-standards": "latest",
    "@hfe/react-cli-plugin-javascript": "latest",
    "@hfe/react-cli-plugin-router": "latest",
    "@hfe/react-cli-plugin-setup": "latest",
    "axios-mock-adapter": "^1.15.0",
    "yorkie": "2.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-configrc.js"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,scss,less,stylus}": [
      "prettier --write",
      "git add"
    ]
  }
}
