{
  "name": "@mt-material/datafe-auth-apply-dynamic",
  "version": "1.0.81",
  "description": "权限申请组件",
  "scripts": {
    "sign": "sc login",
    "bootstrap": "yarn --registry=http://r.npm.sankuai.com && sc edcCliCheck",
    "start": "sc initTemplateJson && sc initComponentDoc && yarn run dev",
    "dev": "webpack serve --config ./node_modules/@mtfe/sc-vue-ts-static/webpack.config.js",
    "release": "npm run lint && yarn build && yarn release:edc && sc autoCommit && sc publish",
    "release:edc": "npm run lint && sc initEdcYml && edc publish --type webpack",
    "publish-package": "sc publish",
    "build": "yarn build:doc && yarn build:src",
    "build:doc": "webpack --config ./node_modules/@mtfe/sc-vue-ts-static/build/webpack.build-doc.config.js",
    "build:src": "webpack --config ./node_modules/@mtfe/sc-vue-ts-static/build/webpack.build-src.config.js && sc createDeclaration",
    "analyz": "NODE_ENV=production npm_config_report=true npm run build",
    "lint": "./node_modules/.bin/eslint src --ext .ts,.js,.vue"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "doc",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@git.sankuai.com/ss/auth-apply-dynamic.git"
  },
  "keywords": [
    "sc-cli-vue-ts"
  ],
  "author": "liuping02",
  "license": "ISC",
  "dependencies": {
    "@datafe/data-report": "^1.0.0",
    "@mtfe/sc-web": "1.0.14",
    "vue-text-highlight": "^2.0.10"
  },
  "devDependencies": {
    "@datafe/config": "0.3.5",
    "@mtfe/sc-commander": "0.0.19",
    "@mtfe/sc-vue-ts-static": "0.0.23",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "@vue/eslint-config-typescript": "5.0.2",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "6.2.2"
  }
}