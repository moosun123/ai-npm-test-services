{
    "name": "将军令权限申请组件",
    "cnName": "将军令权限申请组件",
    "maintainer": "",
    "componentId": "",
    "desc": "提供更优雅的权限申请体验。（基于服务组件化方案提供技术支撑：https://km.sankuai.com/page/368896998）",
    "props": [
        {
            "name": "system",
            "desc": "系统 code（必填）",
            "type": "string",
            "optionalValue": "",
            "default": "-",
            "version": "0.0.1"
        },
        {
            "name": "value",
            "desc": "是否展示弹窗",
            "type": "boolean",
            "optionalValue": "true/false",
            "default": "false",
            "version": "0.0.1"
        },
        {
            "name": "resourceCode",
            "desc": "资源根节点的权限 code（必填）",
            "type": "array",
            "optionalValue": "",
            "default": "-",
            "version": "0.0.1"
        },
        {
            "name": "childRC",
            "desc": "勾选的子节点的资源权限 code 或 名称，传空时默认全勾选（可选）",
            "type": "array",
            "optionalValue": "",
            "default": "[]",
            "version": "0.0.1"
        },
        {
            "name": "viewType",
            "desc": "节点展示的处理逻辑，如果值为 'selectOnly'，则用户只能看到 childRC 中的字段、'selectNull' 为不选中，（可选）",
            "type": "string",
            "optionalValue": "'selectOnly', 'selectNull'",
            "default": "空",
            "version": "1.0.61"
        },
        {
            "name": "misId",
            "desc": "申请人的 misId（必填）",
            "type": "string",
            "optionalValue": "",
            "default": "-",
            "version": "0.0.1"
        },
        {
            "name": "userName",
            "desc": "申请人的展示名称（可选，不填时将直接使用 misid）",
            "type": "string",
            "optionalValue": "",
            "default": "-",
            "version": "0.0.1"
        },
        {
            "name": "checkedAlreadyHaveAuth",
            "desc": "默认勾选已有权限（可选），生效场景：childRC 有值，会默认勾选非 childRC 的已有全部权限的节点",
            "type": "boolean",
            "optionalValue": "",
            "default": "false",
            "version": "1.0.78"
        },
        {
            "name": "modalProps",
            "desc": "透传给 mtd-modal 组件的参数，参照 mtd-vue 官网（可选）",
            "type": "object",
            "optionalValue": "",
            "default": "-",
            "version": "0.0.1"
        },
        {
            "name": "env",
            "desc": "环境，配置此参数，指向不同环境的将军令服务",
            "type": "string",
            "optionalValue": "production、staging、test、development",
            "default": "'production'",
            "version": "0.0.1"
        }
    ],
    "events": [
        {
            "name": "input",
            "desc": "弹窗展示状态变更时的事件，可结合 value 属性，使用 v-model",
            "signature": "",
            "args": "展示状态",
            "version": "0.0.1"
        },
        {
            "name": "status",
            "desc": "用户申请操作的提交状态，可结合进行后续动作",
            "signature": "",
            "args": "'success'/'fail'",
            "version": "0.0.1"
        },
        {
            "name": "initFail",
            "desc": "组件初始化失败的回调",
            "signature": "",
            "args": "-",
            "version": "0.0.1"
        }
    ],
    "methods": [],
    "slots": []
}