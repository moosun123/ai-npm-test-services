{
  "name": "@ss/mtd-vue",
  "version": "0.4.16",
  "description": "A Component Library For Vue.js",
  "author": "wangchenyu02@meituan.com",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.sankuai.com/ss/mtd-vue.git"
  },
  "license": "MIT",
  "main": "lib/mtd-vue.common.js",
  "module": "es/index.js",
  "sideEffects": [
    "lib/*",
    "*.vue",
    "*.css",
    "*.scss",
    "*.md"
  ],
  "files": [
    "lib",
    "es",
    "src",
    "components",
    "types",
    "helper",
    "components.json",
    "script",
    "vetur"
  ],
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "npm run build:file && npm run build:css && webpack-dev-server --inline --progress --config build/webpack.demo.conf.js",
    "build:deploy": "npm run clean && npm run build:css && cross-env NODE_ENV=production webpack --progress --config build/webpack.deploy.conf.js",
    "start": "npm run dev",
    "create": "node build/bin/create.js",
    "unit": "npm run unit:watch -- --single-run",
    "unit:watch": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "test": "npm run unit",
    "clean": "rimraf lib es dist test/**/coverage vetur",
    "copy:types": "cp -r types/*.d.ts lib",
    "check:variables": "./node_modules/.bin/variableAnalyzer verify ./components/theme-chalk/common/_var.scss && ./node_modules/.bin/variableAnalyzer verify ./components/theme2/common/_var.scss ",
    "build": "cross-env NODE_ENV=production npm run build:all",
    "build:all": "npm run clean && npm run build:file && npm run build:component && npm run build:common && npm run build:utils && npm run build:index && npm run build:es && npm run build:css && npm run build:theme && npm run build:types && npm run copy:types",
    "build:file": "node build/bin/build-entry.js",
    "build:types": "node build/bin/build-ts.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:es": "node ./build/compiler/index.js",
    "build:index": "webpack --config build/webpack.umd.conf.js",
    "build:common": "webpack --config build/webpack.build-all.conf.js",
    "build:component": "webpack --config build/webpack.build.conf.js",
    "build:css": "node build/bin/build-css.js",
    "build:theme": "gulp --gulpfile gulpfile.js build",
    "build:theme-chalk": "npm run build:css && gulp --gulpfile gulpfile.js build:theme-chalk",
    "build:theme2": "npm run build:css && gulp --gulpfile gulpfile.js build:theme2",
    "build:gulp3:theme-chalk": "npm run build:css && gulp --gulpfile gulpfile3.js build:theme-chalk",
    "build:gulp3:theme2": "npm run build:css && gulp --gulpfile gulpfile3.js build:theme2",
    "lint": "eslint --ext .js,.vue src components examples",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "postinstall": "node script/report.js",
    "release": "node script/release.js"
  },
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "dependencies": {
    "@ss/lx-report": "^0.0.3-beta.5",
    "async-validator": "^1.12.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-runtime": "^6.26.0",
    "dayjs": "^1.10.7",
    "deepmerge": "^2.2.1",
    "normalize-wheel": "^1.0.1",
    "popper.js": "^1.16.0",
    "resize-observer-polyfill": "^1.5.1",
    "throttle-debounce": "^1.0.1"
  },
  "devDependencies": {
    "@bfe/mtd-variables-analyzer": "^0.1.1-beta.1",
    "@bfe/vue-source-doc-loader": "0.0.9",
    "@mosfe/beam-plugin-s3plus": "^0.1.10",
    "@roo/pizza-import": "^0.2.3",
    "@vue/test-utils": "1.0.0-beta.28",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.3.2",
    "dirty-chai": "^2.0.1",
    "enquirer": "^2.3.6",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-postcss": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "js-cookie": "^2.2.1",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^8.2.1",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^3.2.0",
    "nanoid": "^3.1.9",
    "node-notifier": "^5.4.3",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^3.2.1",
    "ora": "^1.2.0",
    "portfinder": "^1.0.25",
    "postcss-loader": "^3.0.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^2.7.1",
    "sass": "1.39.0",
    "sass-loader": "^10.2.0",
    "semver": "^7.3.2",
    "shelljs": "^0.7.6",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.3.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^3.0.0",
    "vue": "^2.6.10",
    "vue-highlight.js": "^3.1.0",
    "vue-loader": "^15.7.2",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^3.1.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.10",
    "vue-text-highlight": "^2.0.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "lint-staged": {
    "{src,examples,components}/**/*.{js,vue}": [
      "npm run lint"
    ]
  },
  "directories": {
    "example": "examples",
    "test": "test"
  }
}
