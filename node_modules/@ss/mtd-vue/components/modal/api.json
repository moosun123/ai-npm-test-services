{
  "props": [
    {
      "name": "mask",
      "desc": "是否需要遮罩层",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "mask-closable",
      "desc": "点击蒙层是否允许关闭",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "enable-click-through",
      "desc": "是否允许点击穿透，为 true 时节点将不在覆盖整个视口，点外部关闭弹框功能将失效，允许用户点击弹框下方内容。",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": "0.3.20"
    },
    {
      "name": "closable",
      "desc": "是否显示关闭按钮",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "fullscreen",
      "desc": "是否全屏",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "visible",
      "desc": "<strong>受控</strong> 是否显示，支持 v-model",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "destroy-on-close",
      "desc": "是否在关闭时销毁 Modal 里的子元素",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "lock-scroll",
      "desc": "是否在 Modal 出现时将 body 滚动锁定",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "title",
      "desc": "Modal 的标题，也可通过具名 slot （见下表）传入",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "placement",
      "desc": "位置",
      "type": "string",
      "optionalValue": [
        "top",
        "center"
      ],
      "default": "center",
      "version": ""
    },
    {
      "name": "width",
      "desc": "宽度 (当为 string 时需要带上单位)",
      "type": [
        "string",
        "number"
      ],
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "append-to-container",
      "desc": "是否将弹出框插入至容器元素中。在弹出框的定位出现问题时，可将该属性设置为 false",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "get-popup-container",
      "desc": "获取容器元素方法，需要返回 HTMLElement 对象",
      "type": "function (): HTMLElement",
      "optionalValue": "",
      "default": "默认返回 document.body",
      "version": ""
    },
    {
      "name": "mount-on-create",
      "desc": "是否在创建时初始化内部元素",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": "0.3.1"
    },
    {
      "name": "keyboard",
      "desc": "是否支持键盘 esc 关闭",
      "type": "boolean",
      "optionalValue": "",
      "default": "",
      "version": "0.3.34"
    }
  ],
  "events": [
    {
      "name": "input",
      "desc": "当用户操作触发 visible 发生改变时，可在此事件中阻止 visible 属性值变更来达到阻止 modal 组件关闭的效果",
      "args": "function (visible: boolean)"
    },
    {
      "name": "open",
      "desc": "打开弹出框时触发。",
      "args": "function()",
      "version": "0.1.40"
    },
    {
      "name": "close",
      "desc": "点击遮罩层或右上角叉的事件",
      "args": "function()"
    },
    {
      "name": "closed",
      "desc": "已关闭 modal 的回调事件",
      "args": "function()"
    }
  ],
  "slots": [
    {
      "name": "default",
      "desc": "内容区"
    },
    {
      "name": "title",
      "desc": "标题"
    },
    {
      "name": "footer",
      "desc": "底部内容"
    }
  ]
}