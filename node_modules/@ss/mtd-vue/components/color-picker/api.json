{
  "props": [
    {
      "name": "value",
      "desc": "绑定值，支持 v-model",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "disabled",
      "desc": "是否禁用",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "size",
      "desc": "尺寸",
      "type": "string",
      "optionalValue": ["不设置", "small", "mini"],
      "default": "不设置",
      "version": ""
    },
    {
      "name": "show-alpha",
      "desc": "是否支持透明度选择",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "color-format",
      "desc": "写入value的颜色的格式",
      "type": "string",
      "optionalValue": ["hsl", "hsv", "hex", "rgb"],
      "default": "hex（show-alpha 为 false）/rgb（show-alpha 为 true）",
      "version": ""
    },
    {
      "name": "popper-class",
      "desc": "ColorPicker 下拉框的类名",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "predefine",
      "desc": "预定义颜色",
      "type": "Array",
      "optionalValue": "",
      "default": "",
      "version": ""
    }
  ],
  "events": [
    {
      "name": "update:visible",
      "desc": "当颜色面板显示状态发生改变时触发，参数为下拉状态.",
      "args": "(visible: boolean)",
      "version": "0.1.40"
    },
    {
      "name": "change",
      "desc": "当绑定值变化时触发",
      "args": "(value: string)"
    },
    {
      "name": "active-change",
      "desc": "面板中当前显示的颜色发生改变时触发,参数为当前色值",
      "args": "(currentColor:string)"
    }
  ]
}
