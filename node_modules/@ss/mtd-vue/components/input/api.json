{
  "props": [{
    "name": "genre",
    "desc": "类型",
    "type": "string",
    "optionalValue": ["不设置", "area", "line"],
    "default": "不设置",
    "version": ""
  },{
    "name": "size",
    "desc": "大小",
    "type": "string",
    "optionalValue": ["small", "不设置", "large"],
    "default": "不设置",
    "version": ""
  },{
    "name": "clearable",
    "desc": "是否有清空按钮",
    "type": "boolean",
    "optionalValue": "",
    "default": "false",
    "version": ""
  },{
    "name": "invalid",
    "desc": "是否是错误状态",
    "type": "boolean",
    "optionalValue": "",
    "default": "",
    "version": ""
  },{
    "name": "disabled",
    "desc": "是否禁用",
    "type": "boolean",
    "optionalValue": "",
    "default": "false",
    "version": ""
  },{
    "name": "readonly",
    "desc": "是否只读",
    "type": "boolean",
    "optionalValue": "",
    "default": "false",
    "version": ""
  },{
    "name": "value",
    "desc": "输入框的内容",
    "type": "string",
    "optionalValue": "",
    "default": "",
    "version": ""
  },{
    "name": "prefix-icon",
    "desc": "输入框头部图标",
    "type": "string",
    "optionalValue": "",
    "default": "",
    "version": ""
  },{
    "name": "suffix-icon",
    "desc": "输入框尾部图标",
    "type": "string",
    "optionalValue": "",
    "default": "",
    "version": ""
  }, {
    "name": "form-no-validate",
    "desc": "当配置为 true 时当前输入框输入时将不触发表单校验",
    "type": "boolean",
    "optionalValue": "",
    "default": "false",
    "version": "0.3.11"
  }],
  "slots": [{
    "name": "prefix",
    "desc": "输入框头部图标插槽"
  },{
    "name": "suffix",
    "desc": "输入框尾部图标插槽"
  },{
    "name": "prepend",
    "desc": "输入框前置内容"
  },{
    "name": "append",
    "desc": "输入框后置内容"
  }],
  "events": [{
    "name": "input",
    "desc": "当输入值改变时触发",
    "args": "(value: string)"
  },{
    "name": "change",
    "desc": "当输入值改变时触发，0.3.18 后拼音输入过程中将不在触发，如有需要可使用 input 事件",
    "args": "(value: string)"
  },{
    "name": "clear",
    "desc": "当点击清空按钮时触发",
    "args": ""
  },{
    "name": "click-prefix",
    "desc": "点击 prefix-icon 时触发",
    "args": "(event: Event)"
  },{
    "name": "click-suffix",
    "desc": "点击 suffix-icon 时触发",
    "args": "(event: Event)"
  },{
    "name": "enter",
    "desc": "用户按下回车时触发，在中文输入过程中按下回车不会触发",
    "args": "(event: Event)",
    "version": "0.3.5"
  }]
}
