{
  "props": [
    {
      "name": "value",
      "desc": "当前展开的面板值，可以是数组也可以是单个值，当是数组时则表示可以同时展开多个",
      "type": ["any[]", "any"],
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "type",
      "desc": "面板类型",
      "type": "string",
      "optionalValue": ["不设置", "sample", "area"],
      "default": "不设置",
      "version": ""
    }
  ],
  "events": [
    {
      "name": "input",
      "desc": "当切换展开面板时触发",
      "args": "(value: any[] | any)"
    }
  ]
}
