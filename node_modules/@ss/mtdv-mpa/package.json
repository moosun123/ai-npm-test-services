{
    "name": "@ss/mtdv-mpa",
    "version": "0.1.3",
    "description": "A Component Library For Vue.js",
    "repository": {
        "type": "git",
        "url": "ssh://git@git.sankuai.com/ss/mpa.git"
    },
    "license": "MIT",
    "scaffoldVersion": "0.0.10",
    "main": "lib/app.js",
    "module": "lib/app.js",
    "files": [
        "lib",
        "src",
        "components",
        "types",
        "template.json",
        "report.js",
        "template.json",
        "report.js"
    ],
    "typings": "types/index.d.ts",
    "scripts": {
        "sign": "cm login",
        "delete": "cm remove",
        "bootstrap": "yarn --registry=http://r.npm.sankuai.com",
        "start": "cm buildEntry && yarn start:container",
        "build": "export NODE_ENV=production && npm run clean && npm run build:entry && npm run build:components && npm run build:css && npm run build:types",
        "build:esm": "export NODE_ENV=production && npm run clean && npm run build:entry && npm run build:components && npm run build:es && npm run build:css && npm run build:types",
        "touch": "cm create",
        "start:container": "cp node_modules/@ss/business-component-docs/config/vue.start.js ./vue.config.js && vue-cli-service serve node_modules/@ss/business-component-docs/src",
        "release": "yarn test && yarn build && cm publish",
        "clean": "rimraf lib && rimraf components/*/lib && rimraf tests/**/coverage && rimraf examples/mtd-vue",
        "test": "TYPE=run TEST_TYPE=component node ./tests/unit/test",
        "jest-test": "vue-cli-service test:unit --no-cache",
        "build:entry": "cm buildEntry",
        "build:es": "node compiler/index.js",
        "build:components": "cp node_modules/@ss/business-component-docs/config/vue.build.js ./vue.config.js && vue-cli-service build",
        "build:types": "cm buildTS && cp -r types/*.d.ts lib",
        "build:css": "cm buildCSS && gulp --gulpfile gulpfile.js build",
        "build:doc": "export NODE_ENV=production && cp node_modules/@ss/business-component-docs/config/vue.doc.js ./vue.config.js && vue-cli-service build ./node_modules/@ss/business-component-docs/src",
        "postinstall": "node report.js",
        "publish:docs": "cm publish -pdc",
        "publish:lowcode": "cm publish -plw",
        "build:lowcode": "yarn build && cm buildLowcode && cp ./lib/index.js ./lib/lowcode/lib.umd.js",
        "test:head": "TYPE=open node ./tests/unit/test",
        "test:component": "TYPE=run TEST_TYPE=component node ./tests/unit/test",
        "test:component:head": "TYPE=open TEST_TYPE=component node ./tests/unit/test",
        "test:open": "cypress open",
        "lint-fix": "tslint --fix components/**/*.ts -t verbose",
        "report:text": "npx nyc report --reporter=lcov --reporter=text-summary",
        "report:html": "npx nyc report --reporter=lcov --reporter=html"
    },
    "peerDependencies": {
        "@ss/mtd-vue": "^0.4.0",
        "vue": "2.6.11"
    },
    "devDependencies": {
        "@bfe/vue-source-doc-loader": "^0.0.10",
        "@cypress/code-coverage": "^3.10.0",
        "@ss/business-component-docs": "0.0.45",
        "@ss/mtd-vue": "^0.4.0",
        "@ss/vue-component-name-loader": "^0.0.2",
        "@types/chai": "^4.2.6",
        "@types/jest": "^25.1.2",
        "@vue/cli-plugin-babel": "^3.11.0",
        "@vue/cli-plugin-typescript": "^3.11.0",
        "@vue/cli-plugin-unit-jest": "^4.2.2",
        "@vue/cli-service": "^3.11.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^25.1.0",
        "babel-plugin-istanbul": "^6.1.1",
        "chai": "^4.2.0",
        "coveralls": "^3.1.1",
        "cypress": "^10.11.0",
        "gulp": "^4.0.2",
        "gulp-cssmin": "^0.2.0",
        "gulp-dart-sass": "^0.9.1",
        "gulp-postcss": "^8.0.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "istanbul-lib-coverage": "^3.2.0",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.0",
        "sass": "^1.23.7",
        "sass-loader": "^7.3.1",
        "typescript": "4.3.5",
        "uppercamelcase": "^3.0.0",
        "vite-plugin-istanbul": "^3.0.4",
        "vue": "2.6.11",
        "vue-highlight.js": "^3.1.0",
        "vue-loader": "^15.7.1",
        "vue-markdown-loader": "^2.4.1",
        "vue-property-decorator": "^8.2.2",
        "vue-router": "^3.1.3",
        "vue-template-compiler": "2.6.11"
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 5.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "lint-staged": {
        "{src,examples,components}/**/*.{js,vue}": [
            "npm run lint"
        ]
    },
    "directories": {
        "example": "examples",
        "test": "tests"
    },
    "dependencies": {
        "@babel/helper-environment-visitor": "^7.16.7",
        "@datafe/data-report": "^1.0.0",
        "@ss/components-manage-cli": "^1.0.11",
        "@types/lodash": "^4.14.182",
        "@vue/composition-api": "^1.7.1",
        "babel-core": "^7.0.0-bridge.0",
        "dayjs": "^1.11.7",
        "lodash": "^4.17.21",
        "pinyin-pro": "^3.4.1",
        "popper.js": "^1.16.1"
    },
    "optionalDependencies": {
        "vite": "3.1.8",
        "vite-plugin-vue2": "^2.0.2"
    }
}
