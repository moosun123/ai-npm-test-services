[
    {
        "name": "mpa-city-select",
        "cnName": "城市选择器",
        "maintainer": "zhangzhenglong02",
        "componentId": "MlnuvOI_F3R980qI_MlpU",
        "desc": "",
        "props": [
            {
                "name": "value",
                "desc": "支持 v-model，表示组件当前选中项",
                "type": "array",
                "optionalValue": "-",
                "default": "[]",
                "version": ""
            },
            {
                "name": "visible",
                "desc": "非必须，是否显示下拉popover",
                "type": "boolean",
                "optionalValue": "-",
                "default": "-",
                "version": ""
            },
            {
                "name": "data",
                "desc": "数据源",
                "type": "array",
                "optionalValue": "-",
                "default": "[]",
                "version": ""
            },
            {
                "name": "use-default-data",
                "desc": "是否启用默认数据源",
                "type": "boolean",
                "optionalValue": "-",
                "default": "true",
                "version": ""
            },
            {
                "name": "show-select-all",
                "desc": "是否显示全国",
                "type": "boolean",
                "optionalValue": "-",
                "default": "true",
                "version": ""
            },
            {
                "name": "data-props",
                "desc": "自定义数据源字段名",
                "type": "object",
                "optionalValue": "-",
                "default": "{ \"label\": \"label\", \"value\": \"value\", \"children\": \"children\", \"disabled\": \"disabled\" }",
                "version": ""
            },
            {
                "name": "data-source-type",
                "desc": "数据源聚合类型",
                "type": "string",
                "optionalValue": "regions|level|sequence|bussiness",
                "default": "regions",
                "version": ""
            },
            {
                "name": "leaf-level",
                "desc": "叶子节点层级，根据层级，渲染不同UI",
                "type": "number",
                "optionalValue": "-",
                "default": "2",
                "version": ""
            },
            {
                "name": "genre",
                "desc": "选择框类型 (同 Input 组件)",
                "type": "string",
                "optionalValue": "不设置 / area",
                "default": "不设置",
                "version": ""
            },
            {
                "name": "select-any-level",
                "desc": "是否允许选择任意一级的选项，数据源为组件兜底数据或自定义市结构时，禁用该属性",
                "type": "boolean",
                "optionalValue": "-",
                "default": "false",
                "version": ""
            },
            {
                "name": "clearable",
                "desc": "是否显示清除按钮",
                "type": "boolean",
                "optionalValue": "-",
                "default": "false",
                "version": ""
            },
            {
                "name": "multiple",
                "desc": "是否多选",
                "type": "boolean",
                "optionalValue": "-",
                "default": "false",
                "version": ""
            },
            {
                "name": "placeholder",
                "desc": "选择框占位符",
                "type": "string",
                "optionalValue": "-",
                "default": "选择城市",
                "version": ""
            },
            {
                "name": "formatter",
                "desc": "格式化输入框中显示的内容，multiple=false时生效，其中labels 为当前匹配到的 label 数组，selectedOptions 为当前选中的option数组，vlaue为当前选中的值；",
                "type": "function(labels, selectedOptions, value): string",
                "optionalValue": "-",
                "default": "-",
                "version": ""
            },
            {
                "name": "separator",
                "desc": "选项分割符，未配置 formatter 属性时生效",
                "type": "string",
                "optionalValue": "-",
                "default": "/",
                "version": ""
            },
            {
                "name": "display-category",
                "desc": "是否显示类别选项卡",
                "type": "boolean",
                "optionalValue": "-",
                "default": "true",
                "version": ""
            },
            {
                "name": "category-position",
                "desc": "类别选项卡位置，display-category为true时生效",
                "type": "string",
                "optionalValue": "inside|outside",
                "default": "inside",
                "version": ""
            },
            {
                "name": "category-data",
                "desc": "类别选项卡数据源，display-category为true时生效",
                "type": "array",
                "optionalValue": "-",
                "default": "[{\"label\":\"按行政区\",\"value\":\"regions\"},{\"label\":\"按音序\",\"value\":\"sequence\"}]",
                "version": ""
            },
            {
                "name": "loading",
                "desc": "数据源是否为加载中状态",
                "type": "boolean",
                "optionalValue": "-",
                "default": "-",
                "version": ""
            },
            {
                "name": "loading-text",
                "desc": "数据源加载时文案",
                "type": "string",
                "optionalValue": "-",
                "default": "正在加载中",
                "version": ""
            },
            {
                "name": "search-text",
                "desc": "搜索框提示文案",
                "type": "string",
                "optionalValue": "-",
                "default": "搜索省份/城市",
                "version": ""
            },
            {
                "name": "append-to-body",
                "desc": "是否将弹出框插入至 body 元素。在弹出框的定位出现问题时，可将该属性设置为 true",
                "type": "boolean",
                "optionalValue": "-",
                "default": "false",
                "version": ""
            },
            {
                "name": "popper-class",
                "desc": "下拉框类名",
                "type": "string",
                "optionalValue": "-",
                "default": "-",
                "version": ""
            },
            {
                "name": "blur-auto-save",
                "desc": "失去焦点时，是否自动保存",
                "type": "boolean",
                "optionalValue": "-",
                "default": "false",
                "version": ""
            }
        ],
        "events": [
            {
                "name": "change",
                "desc": "绑定值发生变化时触发",
                "signature": "",
                "args": "function(value: array, data: array, labels: array)",
                "version": ""
            },
            {
                "name": "update:visible",
                "desc": "下拉框显示／隐藏状态变化时触发",
                "signature": "",
                "args": "function(visible: boolean)",
                "version": ""
            },
            {
                "name": "category-change",
                "desc": "类别选项卡切换时触发，参数是当前选中类别的唯一标识",
                "signature": "",
                "args": "function(value: string)",
                "version": ""
            }
        ],
        "methods": [
            {
                "name": "focus",
                "desc": "使组件获取焦点",
                "signature": "",
                "args": "-",
                "version": ""
            },
            {
                "name": "blur",
                "desc": "使组件是去焦点",
                "signature": "",
                "args": "-",
                "version": ""
            }
        ],
        "slots": [
            {
                "name": "recentView",
                "desc": "显示常用的或最近选择过的地区，显示位置在搜索框下方、内容区域上方",
                "version": ""
            },
            {
                "name": "category",
                "desc": "自定义外置型类别选项卡",
                "version": ""
            }
        ]
    }
]
