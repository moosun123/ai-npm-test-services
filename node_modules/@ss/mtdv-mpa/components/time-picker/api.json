{
  "props": [
    {
      "name": "size",
      "desc": "设置输入框的大小，同 Input 组件 size 属性",
      "type": "string",
      "optionalValue": [
        "small",
        "不设置",
        "large"
      ],
      "default": "不设置 ",
      "version": ""
    },
    {
      "name": "type",
      "desc": "显示类型, 0.3.9 版本开始支持 timerange",
      "type": "string",
      "optionalValue": "",
      "default": [
        "time",
        "timerange"
      ],
      "version": ""
    },
    {
      "name": "format",
      "desc": "展示的日期格式",
      "type": "string",
      "optionalValue": "",
      "default": "HH:mm:ss",
      "version": ""
    },
    {
      "name": "editable",
      "desc": "文本框是否可输入",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "readonly",
      "desc": "是否只读",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "disabled",
      "desc": "是否禁用",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "clearable",
      "desc": "是否可清空",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "open",
      "desc": "非必须，同 DatePicker，是否显示下方面板，支持 .sync",
      "type": "boolean",
      "optionalValue": "",
      "default": "false",
      "version": ""
    },
    {
      "name": "placeholder",
      "desc": "空内容时的占位内容",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "placement",
      "desc": "弹出面板默认的方向",
      "type": "string",
      "optionalValue": [
        "top",
        "top-start",
        "top-end",
        "bottom",
        "bottom-start",
        "bottom-end",
        "left",
        "left-start",
        "left-end",
        "right",
        "right-start",
        "right-end"
      ],
      "default": "bottom-start",
      "version": " "
    },
    {
      "name": "name",
      "desc": "输入框的 name 属性",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "value",
      "desc": "当前选中的值，当多选或者时间段时为数组",
      "type": [
        "Date",
        "Date[]"
      ],
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "value-format",
      "desc": "绑定值的格式。不指定则根据当前 value 值的类型进行推断",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": "0.1.44"
    },
    {
      "name": "size",
      "desc": "输入框的尺寸",
      "type": "string",
      "optionalValue": [
        "small",
        "不设置",
        "large"
      ],
      "default": "不设置",
      "version": ""
    },
    {
      "name": "steps",
      "desc": "下拉列表的时间间隔，数组的三项分别对应小时、分钟、秒。例如设置为 [1, 15] 时，分钟会显示：00、15、30、45",
      "type": "Array",
      "optionalValue": "",
      "default": "[]",
      "version": ""
    },
    {
      "name": "genre",
      "desc": "类型 (同 Input 组件)",
      "type": "string",
      "optionalValue": [
        "不设置",
        "area",
        "line"
      ],
      "default": "不设置",
      "version": ""
    },
    {
      "name": "disabled-hours",
      "desc": "禁用小时, 0.3.23 版本开始支持 function",
      "type": "Number[] / function (): Number[]",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "disabled-minutes",
      "desc": "禁用分钟, 0.3.23 版本开始支持 function",
      "type": "Number[] / function(selectedHour?: number) : Number[]",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "disabled-seconds",
      "desc": "禁用秒, 0.3.23 版本开始支持 function",
      "type": "Number[] / function(selectedHour?: number,selectedMinute?: number) : Number[]",
      "optionalValue": "",
      "default": "",
      "version": ""
    },
    {
      "name": "popper-class",
      "desc": "下拉框的类名",
      "type": "string",
      "optionalValue": "",
      "default": "",
      "version": " "
    },
    {
      "name": "append-to-container",
      "desc": "是否将下拉框插入至容器元素中。在下拉框的定位出现问题时，可将该属性设置为 false",
      "type": "boolean",
      "optionalValue": "",
      "default": "true",
      "version": ""
    },
    {
      "name": "get-popup-container",
      "desc": "获取容器元素方法，需要返回 HTMLElement 对象",
      "type": "function():HTMLElement",
      "optionalValue": "",
      "default": "默认返回 document.body",
      "version": ""
    },
    {
      "name": "popper-options",
      "desc": "自定义 popper.js 的配置项，具体配置查看 <a target='_blank' href='https://popper.js.org/docs/v1/#modifiers--codeobjectcode'>popper.js modifiers 配置项文档</a>",
      "type": "object",
      "optionalValue": "",
      "default": "",
      "version": "0.3.1"
    }
  ],
  "events": [
    {
      "name": "update:open",
      "desc": "当面板显示隐藏时触发",
      "signature": "function (open: boolean)",
      "args": ""
    },
    {
      "name": "input",
      "desc": "当前 value 发生改变时触发",
      "signature": "function (value: date)",
      "args": ""
    },
    {
      "name": "clear",
      "desc": "当点击清除按钮时触发(之后会触发一次 value 为空的 input 事件)",
      "signature": "function ()",
      "args": ""
    }
  ]
}